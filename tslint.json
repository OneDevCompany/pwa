{
  "rules": {
    "align": [
      false
    ],
    "arrow-parens": false,
    "ban": false,
    "class-name": true,
    "comment-format": [
      false
    ],
    "curly": false,
    "eofline": false,
    "export-name": false,
    "forin": true,
    "import-name": false,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": [
      false
    ],
    "jsdoc-format": false,
    "label-position": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "max-file-line-count": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-access": false,
    "member-ordering": [
      false
    ],
    "missing-jsdoc": false,
    "new-parens": true,
    "no-angle-bracket-type-assertion": false,
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [
      true
    ],
    "no-console": [
      true,
      "debug",
      "info",
      "log",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-default-export": false,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-empty-line-after-opening-brace": false,
    "no-eval": true,
    "no-for-in-array": false,
    "no-inferrable-types": [
      false
    ],
    "no-internal-module": false,
    "no-invalid-this": true,
    "no-mergeable-namespace": false,
    "no-missing-visibility-modifiers": false,
    "no-multiline-string": false,
    "no-namespace": false,
    "no-null-keyword": false,
    "no-reference": true,
    "no-require-imports": false,
    "no-reserved-keywords": false,
    "no-shadowed-variable": false,
    "no-sparse-arrays": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unsupported-browser-code": false,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-imports": false,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": [
      false,
      "as-needed"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "one-variable-per-declaration": [
      true
    ],
    "only-arrow-functions": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": false,
    "restrict-plus-operands": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "switch-default": false,
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "always"
      }
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "typedef-whitespace": [
      false
    ],
    "use-isnan": true,
    "variable-name": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "adjacent-overload-signatures": false,
    "array-type": [
      false
    ],
    "completed-docs": [
      false
    ],
    "cyclomatic-complexity": [
      false
    ],
    "file-header": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "no-parameter-properties": false,
    "object-literal-shorthand": false,
    "prefer-for-of": false
  }
}
